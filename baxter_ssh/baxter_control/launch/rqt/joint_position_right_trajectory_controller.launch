<launch>

  <!-- Load RQT with a pre-setup GUI for Baxter controls from a perspective file  -->
  <node name="$(anon baxter_rqt)" pkg="rqt_gui" type="rqt_gui" respawn="false"
	output="screen" args="--perspective-file $(find baxter_control)/launch/rqt/joint_position_right_trajectory_controller.perspective"/>
  
</launch>