<?xml version="1.0"?>
<package>
  <name>baxter_control</name>
  <version>0.3.0</version>
  <description>ros_control controllers for use with Baxter</description>


  <maintainer email="davetcoleman@gmail.com">Dave Coleman</maintainer>

  <author>Dave Coleman</author>
  <license>BSD</license>

  <url type="website">https://github.com/davetcoleman/baxter</url>
  <url type="bugtracker">https://github.com/davetcoleman/baxter_cpp/issues</url>
  <url type="repository">https://github.com/davetcoleman/baxter</url>

  <buildtool_depend>catkin</buildtool_depend>

  <build_depend>std_msgs</build_depend>
  <build_depend>roscpp</build_depend>
  <build_depend>baxter_core_msgs</build_depend>
  <build_depend>controller_manager</build_depend>
  <build_depend>transmission_interface</build_depend>
